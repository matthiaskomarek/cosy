{
  "name": "<%= packageName %>",
  "version": "0.0.0",
  "main": "./dist/<%= packageBundleName %>.umd.js",
  "module": "./dist/<%= packageBundleName %>.esm.js",
  "es2015": "index.js",
  "scripts": {
    "lint:styles": "stylelint './component/**/*.css'",
    "lint": "lint:styles",
    "prebuild": "npm run lint",
    "build": "cross-env BABEL_ENV=build node ../../build.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "README.md",
    "package.json",
    "dist/**",
    "index.js",
    "component/**"
  ],
  "cosyMeta": {
    <%_ if(useProjectSwitch){ _%>
    "type": "<%= atomicType %>",
    "projects": <%- allowedProjects %>
    <%_ } else{ _%>
    "type": "<%= atomicType %>"
    <%_ } _%>
  }
}
